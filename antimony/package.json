    "vue-cli-plugin-tauri": "^0.11.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {}
  },
  "prettier": {
    "trailingComma": "none",
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
