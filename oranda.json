{
  "build": {
    "additional_pages": {
      "Benchmarks": "./BENCHMARKS.md",
      "Contributing": "./CONTRIBUTING.md"
    },
    "static_dir": "assets"
  },
  "styles": {
    "favicon": "./assets/favicon.ico"
  },
  "components": {
    "funding": {
      "preferred_funding": "github"
    },
    "mdbook": {
      "path": "./book"
    },
    "artifacts": {
      "package_managers": {
        "preferred": {
          "npm": "npm install -g orogene",
          "npx": "npx oro -h",
          "crates.io": "cargo install orogene",
          "homebrew": "brew install orogene"
        }
      }
    }
  }
}
